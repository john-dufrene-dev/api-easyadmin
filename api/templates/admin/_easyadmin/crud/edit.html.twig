{% extends '@!EasyAdmin/crud/edit.html.twig' %}

{% trans_default_domain ea.i18n.translationDomain %}

{% block content_title %}
    {%- apply spaceless -%}
        {% set default_title = 'admin.general.title.edit'|trans ~ ea.i18n.translationParameters["%entity_label_singular%"]
            ~ ' : ' ~ ea.crud.defaultPageTitle('edit', entity.instance) %}
        {% set custom_title = ea.crud.customPageTitle('edit', entity.instance) %}
        {{ custom_title is null ? default_title|raw : custom_title|raw }}
    {%- endapply -%}
{% endblock %}
